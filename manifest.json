{
  "description": "Sidebar Tabs",
  "homepage_url": "https://github.com/andymckay/sidebar-tabs",
  "manifest_version": 2,
  "name": "Sidebar Tabs",
  "version": "1.0",
  "sidebar_action": {
    "default_title": "Tabs Sidebar",
    "default_panel": "sidebar.html",
    "default_icon": "error.svg"
  },
  "icons": {
    "48": "error.svg"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
          "default": "Ctrl+Shift+O",
          "mac": "Command+Shift+O"
      }
    }
  },
  "applications": {
      "gecko": {
          "strict_min_version": "54.0a1"
      }
  }
}
