{
  "description": "Sidebar Tabs",
  "homepage_url": "https://github.com/andymckay/sidebar-tabs",
  "manifest_version": 2,
  "name": "Sidebar Tabs",
  "version": "1.0",
  "sidebar_action": {
    "default_title": "Tabs Sidebar",
    "default_panel": "sidebar.html"
  },
  "icons": {
    "48": "sidebar.svg"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
          "default": "Ctrl+Shift+O",
          "mac": "Command+Shift+O"
      }
    }
  }
}
